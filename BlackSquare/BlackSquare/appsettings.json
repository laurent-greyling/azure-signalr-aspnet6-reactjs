{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.SignalR": "Debug"
    }
  },
  "Azure": {
    "SignalR": {
      "ConnectionString" : "Endpoint=https://<signalR_Instance>.service.signalr.net;AccessKey=<Access_Key>;Version=1.0;"
    }
  },
"AllowedHosts": "*"
}
